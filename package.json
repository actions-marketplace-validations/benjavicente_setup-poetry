{
  "name": "poetry-setup",
  "version": "0.1.0",
  "private": true,
  "description": "A GitHub actions that install poetry and cache installation steps",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --license licenses.txt",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "author": "Benjam√≠n Vicente",
  "license": "MIT",
  "dependencies": {
    "@actions/cache": "^1.0.9",
    "@actions/core": "^1.6.0",
    "@actions/exec": "^1.1.0",
    "@actions/glob": "^0.2.0",
    "@actions/io": "^1.1.1",
    "@actions/tool-cache": "^1.7.1"
  },
  "devDependencies": {
    "@types/node": "^16.10",
    "@typescript-eslint/parser": "^5.12",
    "@vercel/ncc": "^0.33",
    "eslint": "^8",
    "eslint-plugin-github": "^4.3",
    "eslint-plugin-jest": "^25.3",
    "jest": "^27.2",
    "js-yaml": "^4.1",
    "prettier": "2.5",
    "ts-jest": "^27.1",
    "typescript": "^4.5"
  }
}
